{
	"name": "importdata",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run",
		"prisma:generate": "prisma generate --schema prisma/cenov/schema.prisma",
		"prisma:migrate": "prisma migrate dev --schema prisma/cenov/schema.prisma",
		"prisma:studio": "prisma studio --schema prisma/cenov/schema.prisma",
		"prisma:push": "prisma db push --schema prisma/cenov/schema.prisma",
		"prisma:pull": "prisma db pull --schema prisma/cenov/schema.prisma",
		"prisma:generate-dev": "prisma generate --schema prisma/cenov_dev/schema.prisma",
		"prisma:migrate-dev": "prisma migrate dev --schema prisma/cenov_dev/schema.prisma",
		"prisma:studio-dev": "prisma studio --schema prisma/cenov_dev/schema.prisma",
		"prisma:push-dev": "prisma db push --schema prisma/cenov_dev/schema.prisma",
		"prisma:pull-dev": "prisma db pull --schema prisma/cenov_dev/schema.prisma",
		"prisma:generate-preprod": "prisma generate --schema prisma/cenov_preprod/schema.prisma",
		"prisma:migrate-preprod": "prisma migrate dev --schema prisma/cenov_preprod/schema.prisma",
		"prisma:studio-preprod": "prisma studio --schema prisma/cenov_preprod/schema.prisma",
		"prisma:push-preprod": "prisma db push --schema prisma/cenov_preprod/schema.prisma",
		"prisma:pull-preprod": "prisma db pull --schema prisma/cenov_preprod/schema.prisma",
		"prisma:generate-all": "pnpm run prisma:generate && pnpm run prisma:generate-dev && pnpm run prisma:generate-preprod",
		"postinstall": "pnpm run prisma:generate-all"
	},
	"devDependencies": {
		"@eslint/compat": "^1.4.1",
		"@eslint/js": "^9.39.1",
		"@internationalized/date": "^3.10.0",
		"@lucide/svelte": "^0.544.0",
		"@sveltejs/adapter-node": "^5.4.0",
		"@sveltejs/kit": "^2.48.5",
		"@sveltejs/vite-plugin-svelte": "^5.1.1",
		"@tailwindcss/vite": "^4.1.17",
		"@testing-library/jest-dom": "^6.9.1",
		"@testing-library/svelte": "^5.2.9",
		"@types/node": "^22.19.1",
		"autoprefixer": "^10.4.22",
		"dotenv": "^17.2.3",
		"eslint": "^9.39.1",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.13.0",
		"flowbite": "^3.1.2",
		"flowbite-svelte": "^0.48.6",
		"flowbite-svelte-icons": "^2.3.0",
		"globals": "^16.5.0",
		"jsdom": "^26.1.0",
		"lucide-svelte": "^0.503.0",
		"mode-watcher": "^1.1.0",
		"postcss": "^8.5.6",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"prisma": "^7.0.0",
		"svelte": "^5.43.12",
		"svelte-check": "^4.3.4",
		"svelte-sonner": "^1.0.6",
		"sveltekit-superforms": "^2.28.1",
		"tailwind-variants": "^1.0.0",
		"tailwindcss": "^4.1.17",
		"ts-node": "^10.9.2",
		"tsx": "^4.20.6",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.47.0",
		"vite": "^6.4.1",
		"vitest": "^3.2.4",
		"zod": "4.1.12"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild",
			"@prisma/client",
			"@prisma/engines"
		],
		"ignoredBuiltDependencies": [
			"prisma"
		]
	},
	"dependencies": {
		"@logto/browser": "^3.0.9",
		"@logto/node": "^3.1.6",
		"@logto/sveltekit": "^0.3.19",
		"@prisma/adapter-pg": "7.0.0",
		"@prisma/client": "^7.0.0",
		"bits-ui": "^2.14.4",
		"clsx": "^2.1.1",
		"csv-parse": "^6.1.0",
		"fast-xml-parser": "^5.3.2",
		"pg": "^8.16.3",
		"svelte-toolbelt": "^0.9.3",
		"tailwind-merge": "^3.4.0",
		"xlsx": "^0.18.5"
	}
}
